	this.$item.on("revert", function (e) {
		that.revertValue.call(that, that.oldValue);
		if (that.setSaving) {
			that.setSaving.call(that, false);
		}
	});
	this.$item.on("saved", function (e) {
		that.oldValue = that.newValue;
		if (that.setSaving) {
