			.off("click", $.proxy(this.onRemoveDocument, this))
			.on("click", $.proxy(this.onRemoveDocument, this));
	} else {
		this.$item.off("change", $.proxy(this.onChangeHandler, this));
		this.$container
			.find(".btn-attach-document")
