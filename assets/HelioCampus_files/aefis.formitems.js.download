	$(".selectpicker").selectpicker("refresh");
};

aefis.FormItems.TERMSELECTION.prototype.setEnabled = function (enable) {
	if (enable) {
		//this.$item.find('.dropdown-toggle').removeClass('disabled');
		this.$item.find("select").prop("disabled", false);
