							return -1;
						} else if (currentUrl == bUrl) {
							return 1;
						} else {
							var aDesc = a.Description.toLowerCase();
							var bDesc = b.Description.toLowerCase();
							return aDesc < bDesc ? -1 : aDesc > bDesc ? 1 : 0;
						}
